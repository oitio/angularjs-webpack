        "bootstrap-sass": "^3.3.0",
        "font-awesome": "^4.7.0",
        "jquery": "^3.2.0"
    },
    "devDependencies": {
        "angular-mocks": "^1.6.0",
        "autoprefixer": "^7.1.0",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.0",
